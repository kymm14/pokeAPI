{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "usesStyledComponents": false,
  "component": {
    "default": {
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false,
      "withIndex": true,
      "customTemplates": {
        "index": "templates/default/index.js",
        "component": "templates/component/TemplateName.jsx"
      }
    },
    "page": {
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false,
      "flat": true,
      "customTemplates": {
        "component": "templates/page/TemplateNamePage.jsx"
      }
    },
    "layout": {
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": true
    }
  }
}